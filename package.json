{
  "name": "react-single-page",
  "version": "1.0.0",
  "description": "## 规范文档：",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "jest  --coverage --colors",
    "precommit": "node ./scripts/pre-commit.js",
    "preparecommitmsg": "node ./scripts/prepare-commit-msg.js",
    "fix": "eslint --fix --ext .jsx,.js ./src/static/js",
    "start": "cross-env NODE_ENV=dev NODE_OPTIONS=--max_old_space_size=102400 webpack-dev-server --progress --hot --config ./webpack/webpack.dev.js",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=204800 webpack  --config ./webpack/webpack.config.js --progress --json > stats.json",
    "release": "cross-env NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/king-king/react-single-page.git"
  },
  "browser": {
    "fs": false,
    "http": false,
    "https": false,
    "node-ensure": false,
    "zlib": false
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "0.1.18",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.4",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^0.28.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16.2": "^1.5.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "happy-git-commit-message-checker": "^1.0.1",
    "happypack": "5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin-for-multihtml": "2.30.2",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "mime-types": "^2.1.18",
    "node-sass": "^4.8.3",
    "node-ensure": "0.0.0",
    "postcss-loader": "^2.1.5",
    "postcss-scss": "^1.0.5",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "worker-loader": "2.0.0",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "highcharts": "6.1.1",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "author": "",
  "license": "ISC"
}
